{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react", // or "react-jsx" for React 17+ with the new JSX runtime
    "allowJs": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "components/ui/BottomNavAnodiam.jsx"  ]
}